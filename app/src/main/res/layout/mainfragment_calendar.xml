<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".mainpage.CalendarFragment">
    <com.applandeo.materialcalendarview.CalendarView
        android:id="@+id/calendarView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:headerColor="?attr/customButtonColor"
        app:headerLabelColor="?android:attr/textColor"
        app:abbreviationsBarColor="?attr/customButtonColor"
        app:abbreviationsLabelsColor="?android:attr/textColor"
        app:pagesColor="?attr/customItemColor"
        app:daysLabelsColor="?android:attr/textColor"/>

</FrameLayout>